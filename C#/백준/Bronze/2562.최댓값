using System.Text;
namespace ConsoleApp4
{
    internal class Program
    {
        static void Main(string[] args)
        {
           

            int[] arr = new int[9];
            for (int j = 0; j < 9; j++)
            {
                arr[j] = int.Parse(Console.ReadLine());
            }
            int A = arr[0];
            int C = 0;
            for (int i = 0; i < 9; i++)
            {

                if (A < arr[i])
                {
                    A = arr[i];
                    C = i + 1;
                }
            }
            if (C == 0)
            {
                C = 1;
            }
            Console.WriteLine(A);
            Console.WriteLine(C);

        }
    }
}


